{
  "name": "test-unitaire",
  "version": "1.0.0",
  "description": "Projet de tests unitaires avec Jest - Approche TDD",
  "main": "module.js",
  "scripts": {
    "test": "jest --coverage",
    "e2e": "cypress run",
    "e2e:open": "cypress open",
    "jsdoc": "jsdoc -c ./jsdoc.config.json -r -d ./docs",
    "start": "webpack serve --mode development --open",
    "build": "webpack --mode production",
    "serve:public": "npx http-server . -p 3001"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.29.0",
    "@babel/preset-react": "^7.18.6",
    "babel-jest": "^30.2.0",
    "jest": "^30.2.0",
    "jsdoc": "^4.0.5",
    "@testing-library/react": "^14.0.0",
    "@testing-library/jest-dom": "^6.0.0",
    "@testing-library/user-event": "^14.4.3",
    "webpack": "^5.88.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.0",
    "babel-loader": "^9.1.2",
    "jest-environment-jsdom": "^30.2.0",
    "cypress": "^12.17.0"
  },
  "peerDependencies": {},
  "optionalDependencies": {},
  "engines": {
    "node": ">=14"
  },
  "jest": {
    "testEnvironment": "jsdom",
    "testPathIgnorePatterns": ["<rootDir>/tests/e2e/", "<rootDir>/cypress/"]
  }
}
